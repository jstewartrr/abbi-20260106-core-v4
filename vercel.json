{
  "redirects": [],
  "rewrites": [
    {
      "source": "/dashboards/executive/jstewart",
      "destination": "/dashboards/executive/jstewart.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*).html",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/email/background-briefing-refresh",
      "schedule": "0 6,10,14,18 * * *"
    }
  ],
  "github": {
    "silent": false
  }
}